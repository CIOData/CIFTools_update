library(tidyverse)
library(tigris)
library(sf)
library(classInt)
library(grid)
library(gridExtra)
library(usmap)
options(tigris_use_cache = TRUE)
stateDf = states(year=2019)
path = 'C:\\Users\\jtburu2\\Dropbox\\UK\\Coursework\\Research\\OTC\\Data\\Ashish\\'
setwd(path)
###NHW by state
df = read.csv('otc_nhw_by_state_sex_fl.csv', header=T) %>%
rename(Years = X5.year.Range.of.Dx..FL.,
State = Addr.at.DX..state,
Rate = Age.Adjusted.Rate,
SE = Standard.Error,
lcl = Lower.Confidence.Interval,
ucl = Upper.Confidence.Interval,
rr = Rate.Ratio,
rrLCL = Ratio.Lower.Confidence.Interval,
rrUCL = Ratio.Upper.Confidence.Interval,
cases = Count) %>%
filter(Years != 'Total',
State != 'Katrina-Rita (pops only)') %>%
dplyr::select(State, Sex, Years, Rate, rr, rrLCL, rrUCL)
#alt
#map incidence rates
dfIR = df %>%
mutate(Sex = factor(Sex, levels = c('Male and female', 'Male', 'Female'))) %>%
filter(Sex != 'Male and female') %>%
rename(state = State)
brks = c(0, 0.5, 1, 1.5, 2, 2.5)
dfIR$brks = cut(dfIR$Rate,
breaks=c(0, 0.5, 1, 1.5, 2, 2.5),
labels=c("0 - 0.49", "0.5 - 0.99", "1 - 1.49",
"1.5 - 1.99", "2 - 2.49"))
p = plot_usmap(data = dfIR, values = 'brks', size = 0.1, color = '#343434') +
scale_fill_brewer(name = 'Incidence Rate', palette = 'OrRd', na.value = 'grey50') +
theme(
plot.title = element_text(hjust = 0.5),
plot.subtitle = element_text(hjust = 0.5),
axis.text.x = element_blank(),
axis.text.y = element_blank(),
axis.ticks = element_blank(),
rect = element_blank()
) +
labs(
x = NULL,
y = NULL,
# title = "OTC Incidence Rate by State"
title = NULL
)
p + facet_grid(col = vars(Sex), row = vars(Years)) +
theme(
legend.position = 'bottom',
legend.direction = 'horizontal',
legend.justification = 'center'
) +
guides(fill = guide_legend(nrow = 1))
p + facet_grid(row = vars(Sex), col = vars(Years)) +
theme(
legend.position = 'bottom',
legend.direction = 'horizontal',
legend.justification = 'center'
) +
guides(fill = guide_legend(nrow = 1))
